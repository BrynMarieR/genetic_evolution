<strategy> ::= lambda h, i: <fixed> | lambda h, i: <adaptive>
<fixed> ::= <varying>[i % 4] | <dove> | <hawk>
<varying> ::= [<choice>, <choice>, <choice>, <choice>]
<hawk> ::= "H"
<dove> ::= "D"
<adaptive> ::= <choice> if i == 0 else (<choice> if h[i-1] == <choice> else <choice>)
<choice> ::= "H" | "D"